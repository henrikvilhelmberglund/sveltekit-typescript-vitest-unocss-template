{
	"name": "sveltekit-typescript-vitest-unocss-template",
	"version": "0.0.0",
	"author": "Henrik Berglund",
	"license": "MIT",
	"description": "SvelteKit template with Typescript, Vitest and UnoCSS",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "npx gh-pages -d build --dotfiles",
		"release": "commit-and-tag-version",
		"postrelease": "git push --follow-tags origin main"
	},
	"devDependencies": {
		"@iconify-json/carbon": "^1.1.21",
		"@julr/unocss-preset-forms": "^0.0.5",
		"@julr/unocss-preset-heropatterns": "^1.0.2",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/svelte": "^4.0.4",
		"@types/jest": "^29.5.7",
		"@unocss/svelte-scoped": "^0.57.1",
		"commit-and-tag-version": "^12.0.0",
		"jsdom": "^22.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unocss": "^0.57.1",
		"unocss-preset-theme": "^0.11.0",
		"vite": "^4.4.2",
		"vitest": "^0.32.2"
	},
	"type": "module"
}
